.appraisal-process-problem-wrap {
  display: none;
  position: fixed;
  top: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.appraisal-process-problem-wrap {
  display: none;
  position: fixed;
  top: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.appraisal-process-problem-wrap .problem-box {
  position: absolute;
  //bottom: -50vh;
  bottom: 0;
  width: 100%;
  height: 12.5rem;
  border-top-left-radius: 0.5rem;
  border-top-right-radius: 0.5rem;
  background: #fff;
  z-index: 9980;
  -webkit-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -ms-transition: all 0.5s;
  -o-transition: all 0.5s;
  transition: all 0.5s;
}
.appraisal-process-problem-wrap .problem-box h1 {
  width: 80%;
  height: 2.5rem;
  margin: 0 auto;
  color: #222;
  font-size: 0.9rem;
  line-height: 2.5rem;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.appraisal-process-problem-wrap .problem-box p {
  width: 90%;
  margin: 0 auto;
  color: #666;
  line-height: 1rem;
  font-size: 0.7rem;
}
.appraisal-process-problem-wrap .problem-close {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100vh;
  z-index: 9900;
}
.appraisal-process {
  width: 100%;
  height: 93vh;
  padding-top: 7vh;
  background: #f6f6f6;
}
.progress-bar {
  position: fixed;
  top: 2.5rem;
  overflow: hidden;
  width: 100%;
  z-index: 9980;
}
.progress-bar .line {
  position: relative;
  display: block;
  height: 0.7rem;
  background: #ffc041;
  -webkit-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -ms-transition: all 0.5s;
  -o-transition: all 0.5s;
  transition: all 0.5s;
}
.progress-bar .after-style {
  position: absolute;
  top: 0;
  right: 1%;
  color: #fff;
  font-size: 0.55rem;
  font-style: normal;
}
.progress-bar .schedule-0 {
  width: 0%;
  height: 0;
}
.progress-bar .schedule-0:after {
  position: absolute;
  top: 0;
  right: 1%;
  color: #fff;
  font-size: 0.55rem;
  font-style: normal;
  content: "";
}
.progress-bar .schedule-01 {
  width: 16.6%;
  height: 0.7rem;
}
.progress-bar .schedule-01:after {
  position: absolute;
  top: 0;
  right: 1%;
  color: #fff;
  font-size: 0.55rem;
  font-style: normal;
  content: "1/6";
}
.progress-bar .schedule-11 {
  width: 33.6%;
  height: 0.7rem;
}
.progress-bar .schedule-11:after {
