.machine-all {
  height: 100vh;
  width: 100%;
  height: 93vh;
  padding-top: 6vh;
  background: #f6f6f6;
}

.machine-all .machine-main {
  width: 100%;
  height: auto;
  padding: 0.5rem 0 3rem;
}
.machine-all .machine-main .machine-tab-box {
  display: none;
}
.machine-all .machine-main .machine-null .calculation-main {
  display: none;
}
.machine-all .machine-main .machine-show {
  display: block;
}
.line {
  border-top: 0.5rem solid #f6f6f6;
}
.machine-all .machine-main .calculation-main {
  width: 94%;
  height: auto;
  padding: 3%;
  background: #fff;
}
.machine-all .machine-main .calculation-main .c-des-list {
  overflow: hidden;
  width: 100%;
  height: auto;
  margin: 0 auto;
}
.machine-all .machine-main .calculation-main .c-des-list h3 {
  position: relative;
  width: 100%;
  height: 1.6rem;
  color: #222;
  line-height: 1.6rem;
  font-size: 0.8rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.machine-all .machine-main .calculation-main .c-des-list ul {
  overflow: hidden;
  margin-top 0.3rem
}
.machine-all .machine-main .calculation-main .c-des-list li {
  width: 100%;
  margin-bottom: 0.5rem;
*zoom: 1;
}
.machine-all .machine-main .calculation-main .c-des-list li .li-left-img {
  float: left;
  width: 27%;
  height: 5.5rem;
}
.machine-all .machine-main .calculation-main .c-des-list li .li-left-img img {
  display: block;
  width: 100%;
  height: 100%;
}
.machine-all .machine-main .calculation-main .c-des-list li .li-right-inf {
  position: relative;
  float: right;
  width: 68%;
}
.machine-all .machine-main .calculation-main .c-des-list li .li-right-inf h4 {
  width: 100%;
  height: 1.8rem;
  color: #222;
  line-height: 1.6rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.machine-all .machine-main .calculation-main .c-des-list li .li-right-inf h4 span {
  float: left;
  width: 70%;
  font-size: 0.85rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.machine-all .machine-main .calculation-main .c-des-list li .li-right-inf h5 {
  width: 100%;
  height: 2.2rem;
  color: #999;
  line-height 1.2rem;
  font-size: 0.7rem;
  overflow: hidden;
}
.machine-all .machine-main .calculation-main .c-des-list li .li-right-inf h6 {
  width: 100%;
  height: 0.8rem;
  color: #999;
  margin-top: 0.5rem;
  font-size: 0.7rem;
}
.machine-all .machine-main .calculation-main .c-des-list li .li-right-inf h6 span {
  font-size: 0.85rem;
}
.machine-all .machine-main .calculation-main .c-des-list li:after {
  content: "\200B";
  display: block;
  height: 0;
  clear: both;
}
.machine-all .machine-null {
  width: 100%;
  padding: 2rem 0;
  text-align:center;
  line-height:45px

}
.machine-all .machine-null img {
  margin-top 30%
  height 35%
  width 50%
}
.machine-all .machine-null a {
  color #4caf50
}
