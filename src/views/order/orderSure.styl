.address-inf-box {
  position: relative;
  display: block;
  width: 100%;
  height: 2.95rem;
  border-radius: 0.5rem;
  margin-top 40px
  background: #fff;
}
.address-inf-box .p-null {
  display: block;
  width: 100%;
  height: 2.95rem;
  color: #28c081;
  font-size: 0.7rem;
  line-height: 2.95rem;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.address-inf-box .p-null a {
  color: #28c081;
}

.c-des-list {
  overflow: hidden;
  width: 90%;
  height: auto;
  margin-left: 6%;
  margin-bottom: 0.5rem;
}
.c-des-list h3 {
  width: 100%;
  height: 1.6rem;
  color: #222;
  line-height: 1.6rem;
  font-size: 0.7rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.c-des-list li {
  width: 100%;
  height: 5.5rem;
  margin-bottom: 0.5rem;
  border-bottom: 1px solid #f6f6f6;
}
.c-des-list li .li-left-img {
  float: left;
  width: 30%;
  height: 4.6rem;
}
.c-des-list li .li-left-img img {
  display: block;
  width: 100%;
  height: 100%;
}
.c-des-list li .li-right-inf {
  position: relative;
  float: right;
  width: 68%;
}
.c-des-list li .li-right-inf h4 {
  width: 100%;
  height: 1rem;
  color: #222;
  line-height: 0.8rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.c-des-list li .li-right-inf h4 span {
  float: left;
  width: 70%;
  font-size: 0.75rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.c-des-list li .li-right-inf h5 {
  width: 100%;
  height: 1rem;
  color: #222;
  font-size: 0.6rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.c-des-list li .li-right-inf h6 {
  width: 100%;
  height: 0.8rem;
  margin-top: 0.6rem;
  font-size: 0.6rem;
}
.c-des-list li .li-right-inf h6 span {
  font-size: 0.8rem;
}
.c-des-list li .li-right-inf div {
  display: block;
  width: 100%;
  height: 1.5rem;
}
.c-des-list li .li-right-inf div p {
  float: left;
  width: 75%;
  height: 1.5rem;
  color: #999;
  line-height: 1.5rem;
  font-size: 0.55rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.c-des-list li .li-right-inf div span {
  float: right;
  width: 25%;
  height: 1.5rem;
  color: #022222;
  text-align: right;
  font-size: 0.8rem;
  line-height: 1.5rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.c-des-list li .li-right-inf div .through {
  font-weight: normal;
  text-decoration: line-through;
}
.c-des-list li .li-right-inf .i-img {
  position: absolute;
  top: 1.9rem;
  left: 0;
  width: 20%;
  height: 0.85rem;
  background: url("/static/image/font_img_01.png") no-repeat;
  -webkit-background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  -ms-background-size: 100% 100%;
  -o-background-size: 100% 100%;
  background-size: 100% 100%;
}
.c-des-list .subtotal-box {
  display: block;
  width: 100%;
  height: 1.85rem;
  color: #222;
  line-height: 1.2rem;
  font-size: 0.7rem;
  text-align: right;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.c-des-list .subtotal-box strong {
  color: #fe4045;
  font-size: 0.8rem;
}

.details-inf {
  width: 93.33%;
  height: auto;
  margin: 0 auto 0.5rem;
  border-radius: 0.5rem;
  background: #fff;
}
.details-inf li {
  width: 92%;
  height: 1.95rem;
  padding: 0 4%;
  border-bottom: 1px solid #f6f6f6;
  line-height: 1.95rem;
}
.details-inf li span {
  float: left;
  width: 50%;
  color: #222;
  font-size: 0.8rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.details-inf li a {
  float: right;
  width: 50%;
  color: #222;
  font-size: 0.7rem;
  text-align: right;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.details-inf li .number-color {
  color: #fe4045;
  font-size: 0.9rem;
}
.details-inf li .right-arrow:after {
  margin-left: 3%;
  content: ">";
  color: #999;
}
.details-inf .rule-p {
  width: 92%;
  height: 1.85rem;
  margin: 0 auto;
  line-height: 1.85rem;
  color: #666;
  font-size: 0.6rem;
}
.details-inf .rule-p span {
  position: relative;
}
.details-inf .rule-p span:after {
  position: absolute;
  top: -0.5rem;
  right: -77%;
  width: 77%;
  height: 1.85rem;
  background: url("/static/image/icon_problem.png") 50% no-repeat;
  -webkit-background-size: 50% 50%;
  -moz-background-size: 50% 50%;
  -ms-background-size: 50% 50%;
  -o-background-size: 50% 50%;
  background-size: 50% 50%;
  content: "";
}

.footer-appraisal {
  position: fixed;
  bottom: 0;
  width: 100%;
  min-width: 320px;
  max-width: 750px;
  height: 2.25rem;
  box-shadow: 0 0 5px 5px #f6f6f6;
  background: #fff;
}
.footer-appraisal span {
  float: left;
  width: 68%;
  height: 2.25rem;
  color: #222;
  font-size: 0.8rem;
  line-height: 2.25rem;
  text-indent: 4%;
}
.footer-appraisal span strong {
  font-weight: normal;
  color: #fe4045;
}
.footer-appraisal span strong b {
  font-size: 0.6rem;
  font-weight: normal;
}
.footer-appraisal a {
  float: right;
  width: 32%;
  height: 2.25rem;
  background: #28c081;
  color: #fff;
  font-size: 0.9rem;
  line-height: 2.25rem;
  text-align: center;
}
