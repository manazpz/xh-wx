}
.confirm-btn {
  display: block;
  width: 100%;
  height: 2.7rem;
  background: #28c081;
}
.confirm-btn .mint-button-text {
  color: #fff;
}
.oldAppraisal .appraisal-main .appraisal-ul .li div {
  position: relative;
  height: 3rem;
}
.oldAppraisal .appraisal-main .appraisal-ul .li div span {
  position : relative;
  float: left;
  width: 40%;
  color: #222;
  overflow auto!important;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.oldAppraisal .appraisal-main .appraisal-ul .li div strong {
  float: right;
  width: 60%;
  color: #28c081;
  text-align: right;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}.oldAppraisal .appraisal-main .appraisal-ul .li div i {
   position: absolute;
   float inside;
   top: 0.7rem;
   left : auto!important;
   width: 18%;
   height: 1.5rem;
   margin-left : 3%;
   background: url(/static/image/icon_problem.png) no-repeat;
   -webkit-background-size: 100% 100%;
   -moz-background-size: 100% 100%;
   -ms-background-size: 100% 100%;
   -o-background-size: 100% 100%;
   background-size: 100% 100%;
 }
.oldAppraisal .appraisal-main .appraisal-ul .li ul li {
  display: block;
  width: 100%;
  height: 2.7rem;
  margin-bottom: 0.57rem;
  border-radius: 0.3rem;
  background: #f6f6f6;
  font-size: 1rem;
  line-height: 2.7rem;
  text-align: center;
}
.appraisal-main .appraisal-ul .li ul {
  display: none;
}
.appraisal-main .appraisal-ul .li .show {
  display: block
}

