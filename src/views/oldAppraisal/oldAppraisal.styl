.layer-load {
  opacity: 0;
}
.top-return {
  position: fixed;
  top: 0;
  width: 100%;
  min-width: 320px;
  max-width: 750px;
  height: 2.35rem;
  background: #28c081;
  z-index: 9998;
}
.appraisal-main {
  width: 100%;
  margin: 0 auto;
}
.appraisal-main .appraisal-ul {
  padding-bottom: 2.4rem;
}
.appraisal-main .appraisal-ul .li div a {
  color: #28c081;
  width: 33%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.appraisal-main .appraisal-ul .li {
  display: block;
  width: 92%;
  padding: 0 4%;
  border-bottom: 1px solid #f6f6f6;
  background: #fff;
  font-size: 1rem;
  line-height: 3rem;
}
.appraisal-main .appraisal-ul .li .show {
  display: block;
}
.appraisal-main .appraisal-ul .li ul .select {
  background: #28c081 !important;
  color: #fff;
}
.offer-btn-box {
  position: fixed;
  bottom: 0;
  width: 100%;
  min-width: 320px;
  max-width: 750px;
  height: 2.8rem;
  color: #fff;
  line-height: 2.8rem;
  text-align: center;
}
.offer-btn-box a {
  display: block;
  width: 100%;
  height: 2.8rem;
  background: #8fdbbb;
  color: #fff;
}
.offer-btn-box .open {
  background: #27c080;
}
.appraisal-process-problem-wrap {
  display: none;
  position: fixed;
  top: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.appraisal-process-problem-wrap {
  display: none;
  position: fixed;
  top: 0;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.appraisal-process-problem-wrap .problem-box {
  position: absolute;
  bottom: -50vh;
  width: 100%;
  height: 12.5rem;
  border-top-left-radius: 0.5rem;
  border-top-right-radius: 0.5rem;
  background: #fff;
  z-index: 9980;
  -webkit-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -ms-transition: all 0.5s;
  -o-transition: all 0.5s;
  transition: all 0.5s;
}
.appraisal-process-problem-wrap .problem-close {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100vh;
  z-index: 9900;
}
.appraisal-process {
  width: 100%;
  height: 93vh;
  padding-top: 7vh;
  background: #f6f6f6;
}
.progress-bar {
  position: fixed;
  top: 2.5rem;
  overflow: hidden;
  width: 100%;
  z-index: 9980;
}
.progress-bar .line {
  position: relative;
  display: block;
  height: 0.7rem;
  background: #ffc041;
  -webkit-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -ms-transition: all 0.5s;
  -o-transition: all 0.5s;
  transition: all 0.5s;
}
.progress-bar .after-style {
  position: absolute;
  top: 0;
  right: 1%;
  color: #fff;
  font-size: 0.55rem;
  font-style: normal;
}
.progress-bar .schedule-0 {
  width: 0%;
  height: 0;
}
.progress-bar .schedule-0:after {
  position: absolute;
  top: 0;
  right: 1%;
  color: #fff;
  font-size: 0.55rem;
  font-style: normal;
  content: "";
}
.progress-bar .schedule-01 {
  width: 16.6%;
  height: 0.7rem;
}
.progress-bar .schedule-01:after {
  position: absolute;
  top: 0;
  right: 1%;
  color: #fff;
  font-size: 0.55rem;
  font-style: normal;
  content: "1/6";
}
.progress-bar .schedule-11 {
  width: 33.6%;
  height: 0.7rem;
}
.progress-bar .schedule-11:after {
