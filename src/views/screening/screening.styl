
.model-search {
  width: 100%;
  padding: 2vh 0;
  background: #f6f6f6;
}
.model-search label {
  position: relative;
  overflow: hidden;
  display: block;
  width: 92%;
  height: 2rem;
  margin: 0 auto;
  padding: 0;
  border-radius: 1rem;
  background: #fff;
}
.model-search label input {
  width: 100%;
  margin: 0;
  padding: 8px;
  text-indent: 10%;
  font-size: 1rem;
}
.model-search label i {
  position: absolute;
  top: 0rem;
  left: 0%;
  width: 9%;
  height: 2rem;
  background: url("/static/image/icon_searchbar.png") 50% no-repeat;
  -webkit-background-size: 65% 65%;
  -moz-background-size: 65% 65%;
  -ms-background-size: 65% 65%;
  -o-background-size: 65% 65%;
  background-size: 65% 65%;
  z-index: 9980;
}
.tab-change-main {
  width: 100%;
  height: 100%;
}
.tab-change-main .left-tab-nav {
  overflow: auto;
  float: left;
  width: 30%;
  height: 100%;
  background: #f6f6f6;
}
.tab-change-main .left-tab-nav li {
  display: block;
  width: 100%;
  height: 3rem;
  background: #f7f7f7;
  color: #666;
  font-size: 1rem;
  line-height: 3rem;
  text-align: center;
}
.tab-change-main .left-tab-nav .select {
  position: relative;
  background: #fff;
  color: #222;
}
.tab-change-main .left-tab-nav .select:after {
  position: absolute;
  top: 0;
  left: 0;
  width: 3%;
  height: 3crem;
  background: #28c081;
  content: "";
  -webkit-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -ms-transition: all 0.5s;
  -o-transition: all 0.5s;
  transition: all 0.5s;
}
.tab-change-main .right-tab-main {
  overflow: auto;
  float: right;
  width: 70%;
  height: 100%;
  background: #fff;
}
.tab-change-main .right-tab-main ul {
  display: none;
}
.tab-change-main .right-tab-main .show {
  display: block;
}
.tab-change-main .right-tab-main li {
  display: block;
  width: 100%;
  height: 3rem;
  border-bottom: 1px solid #f8f8f8;
  background: #fff;
  color: #222;
  font-size: 1rem;
  line-height: 3rem;
  text-align: center;
}
.mint-navbar .mint-tab-item.is-selected {
  padding: 12px 0!important;
  margin-bottom: 0!important;
  border-bottom: 3px solid #28c081!important;
  color: #28c081!important;
}
.navbar
  background-color #0000
  margin-top 40px
.mint-tab-item
  flex initial
  margin-left 0.5rem
.mint-tab-item-label
  font-size 1rem
  padding-left 1rem
  padding-right 1rem

