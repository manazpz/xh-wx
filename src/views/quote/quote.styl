  white-space: nowrap;
  text-overflow: ellipsis;
}
.popup-machine-revise-wrap .machine-revise-box .r-title .revise-cancel-btn {
  position: absolute;
  top: 0;
  left: 3%;
  font-size: 0.8rem;
}
.popup-machine-revise-wrap .machine-revise-box .r-title .revise-confirm-btn {
  position: absolute;
  top: 0;
  right: 3%;
  font-size: 0.8rem;
}
.popup-machine-revise-wrap .machine-revise-box .revise-inf-box {
  display: block;
  width: 100%;
}
.popup-machine-revise-wrap .machine-revise-box .revise-inf-box .li {
  display: block;
  width: 92%;
  padding: 0 4%;
  border-bottom: 1px solid #f6f6f6;
  background: #fff;
  font-size: 0.8rem;
  line-height: 2.25rem;
}
.popup-machine-revise-wrap .machine-revise-box .revise-inf-box .li div {
  height: 2.25rem;
}
.popup-machine-revise-wrap .machine-revise-box .revise-inf-box .li div span {
  position: relative;
  float: left;
  width: 33%;
  color: #222;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.popup-machine-revise-wrap .machine-revise-box .revise-inf-box .li div strong {
  float: right;
  width: 67%;
  color: #222;
  text-align: right;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.popup-machine-revise-wrap .machine-revise-box .revise-inf-box .li ul {
  display: none;
}
.popup-machine-revise-wrap .machine-revise-box .revise-inf-box .li ul li {
  display: block;
  width: 100%;
  height: 2.25rem;
  margin-bottom: 0.57rem;
  border-radius: 0.3rem;
  background: #f6f6f6;
  font-size: 0.8rem;
  line-height: 2.25rem;
  text-align: center;
}
.popup-machine-revise-wrap .machine-revise-box .revise-inf-box .li ul .select {
  background: #28c081;
  color: #fff;
}
.popup-machine-revise-wrap .machine-revise-box .revise-inf-box .li .show {
  display: block;
}
.popup-machine-revise-wrap .machine-revise-box .green-font .li div strong {
  color: #28c081;
}
