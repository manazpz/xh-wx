  border-bottom: 1px solid #f6f6f6;
}
.calculation-main .c-des-list li .li-left-img {
  float: left;
  width: 20%;
  height: 3.2rem;
}
.calculation-main .c-des-list li .li-left-img img {
  display: block;
  width: 100%;
  height: 100%;
}
.calculation-main .c-des-list li .li-right-inf {
  float: right;
  width: 78%;
  height: 3.2rem;
}
.calculation-main .c-des-list li .li-right-inf h4 {
  width: 100%;
  height: 1rem;
  color: #222;
  line-height: 0.8rem;
