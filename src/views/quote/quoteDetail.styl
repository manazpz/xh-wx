.top-return {
  position: fixed;
  top: 0;
  width: 100%;
  min-width: 320px;
  max-width: 750px;
  height: 2.35rem;
  background: #28c081;
  z-index: 9998;
}
.top-inf-appraisal {
  width: 100%;
  height: 5.08rem;
  padding-top: 3.5rem;
  margin-bottom: 0.5rem;
  margin-bottom: 0.5rem;
  background: #fff;
}
.top-inf-appraisal .left-img-box {
  float: left;
  width: 22%;
  height: 5.2rem;
  margin-left: 3.2%;
}
.top-inf-appraisal .left-img-box img {
  display: block;
  width: 100%;
  height: 100%;
}
.top-inf-appraisal .right-inf-box {
  float: right;
  width: 66.67%;
  height: 4.5rem;
  margin-right: 3.2%;
}
.top-inf-appraisal .right-inf-box h2 {
  width: 100%;
  height: 1.2rem;
  color: #222;
  line-height: 1rem;
  font-size: 0.9rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.top-inf-appraisal .right-inf-box h2 span {
  float: left;
  width: 70%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.top-inf-appraisal .right-inf-box h2 span i {
  font-style: normal;
}
.top-inf-appraisal .right-inf-box h2 a {
  float: right;
  color: #666;
  font-size: 0.6rem;
}
.top-inf-appraisal .right-inf-box h3 {
  width: 100%;
  height: 1.2rem;
  color: #222;
  font-size: 0.8rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.top-inf-appraisal .right-inf-box div {
  display: block;
  width: 100%;
  height: 1.5rem;
  padding-top: 1rem;
}
.top-inf-appraisal .right-inf-box div p {
  float: left;
  width: 70%;
  height: 1.5rem;
  color: #999;
  line-height: 2rem;
  font-size: 0.55rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.top-inf-appraisal .right-inf-box div strong {
  float: right;
  width: 30%;
  height: 1.5rem;
  color: #fe4045;
  text-align: right;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.top-inf-appraisal .right-inf-box div strong em {
  font-size: 0.55rem;
  font-weight: normal;
}
.top-inf-appraisal .right-inf-box div strong span {
  font-size: 1.15rem;
}
.calculation-main {
  width: 100%;
  height: auto;
  background: #fff;
}
.calculation-main h2 {
  display: block;
  width: 93%;
  height: 1.75rem;
  margin: 0 auto;
  color: #222;
  line-height: 1.75rem;
  font-size: 0.8rem;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.calculation-main .c-des-list {
