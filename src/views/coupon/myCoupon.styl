.coupons-tab {
  position: fixed;
  top: 2.5rem;
  width: 100%;
  height: 2.5rem;
  background: #fff;
  z-index: 9980;
}
.coupons-tab a {
  position: relative;
  float: left;
  display: block;
  width: 33.2%;
  height: 2.5rem;
  color: #666;
  font-size: 0.8rem;
  line-height: 2.5rem;
  text-align: center;
}
.coupons-tab .select {
  color: #28c081;
}
.coupons-tab .select::after {
  position: absolute;
  bottom: 0;
  left: 25%;
  width: 50%;
  height: 3px;
  background: #28c081;
  content: "";
}
.coupons-box {
  width: 93.6%;
  margin: 2rem auto 0;
  padding-top: 3.5rem;
}
.coupons-box .cou-main {
  display: none;
}
.coupons-box .cou-main .redeem-code {
  width: 100%;
  height: 2rem;
}
.coupons-box .cou-main .redeem-code label {
  position: relative;
  float: left;
  width: 68%;
  height: 1.65rem;
  margin: 0;
  padding: 0;
}
.coupons-box .cou-main .redeem-code label input {
  display: block;
  width: 100%;
  height: 1.85rem;
  border-radius: 1rem;
  background: #fff;
  line-height: 1.85rem;
  text-indent: 5%;
  color: #e5e5e5;
  font-size: 0.8rem;
}
.coupons-box .cou-main .redeem-code label i {
  position: absolute;
  top: 0.5rem;
  right: 3%;
  width: 6.88%;
  height: 1rem;
  background: url("/static/image/coupons_button_exclamationmark@2x.png") no-repeat;
  -webkit-background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  -ms-background-size: 100% 100%;
  -o-background-size: 100% 100%;
  background-size: 100% 100%;
}
.coupons-box .cou-main .redeem-code .redeem-btn {
  float: right;
  width: 23.07%;
  height: 1.8rem;
  border-radius: 1rem;
  background: #28c081;
  color: #fff;
  font-size: 0.8rem;
  line-height: 1.8rem;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.coupons-box .cou-main .title-h2-des {
  position: fixed;
  position: relative;
  display: block;
  width: 100%;
  height: 1.7rem;
  border-radius: 0.2rem;
  background: #fff;
  color: #222;
  line-height: 1.7rem;
  font-size: 0.65rem;
}
.coupons-box .cou-main .title-h2-des span {
  margin-left: 3%;
}
.coupons-box .cou-main .title-h2-des b {
  position: absolute;
  top: 0;
  right: 0;
  width: 10%;
  height: 1.7rem;
  background: url("/static/image/coupons_button_tick_press3.png") 50% no-repeat;
  -webkit-background-size: 50% 50%;
  -moz-background-size: 50% 50%;
  -ms-background-size: 50% 50%;
  -o-background-size: 50% 50%;
  background-size: 50% 50%;
}
.coupons-box .cou-main .title-h2-des .b-hook {
  background: url("/static/image/coupons_button_tick_press.png") 50% no-repeat;
  -webkit-background-size: 50% 50%;
  -moz-background-size: 50% 50%;
  -ms-background-size: 50% 50%;
  -o-background-size: 50% 50%;
  background-size: 50% 50%;
}
.coupons-box .cou-main ul {
  width: 100%;
  height: auto;
  padding: 0 0 17rem;
  background: #f6f6f6;
}
.coupons-box .cou-main li {
  overflow: hidden;
  width: 100%;
  height: 5.8rem;
  margin-bottom: 0.5rem;
  background: url('/static/image/coiling_color_bg_00.png') no-repeat;
  -webkit-background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  -ms-background-size: 100% 100%;
  -o-background-size: 100% 100%;
  background-size: 100% 100%;
}
.coupons-box .cou-main li dl {
  width: 100%;
  height: 5.8rem;
}
.coupons-box .cou-main li dt {
  float: left;
  width: 28.49%;
  height: 5.8rem;
}
.coupons-box .cou-main li dt strong {
  display: block;
  width: 100%;
  height: 3.5rem;
  color: #fff;
  line-height: 4rem;
  font-size: 1.75rem;
  text-align: center;
}
.coupons-box .cou-main li dt strong i {
  font-style: normal;
  font-size: 0.9rem;
}
.coupons-box .cou-main li dt span {
  display: block;
  width: 100%;
  color: #fefefe;
  font-size: 0.75rem;
  text-align: center;
}
.coupons-box .cou-main li dd {
  position: relative;
  float: right;
  width: 67%;
  height: 5rem;
}
.coupons-box .cou-main li dd h4 {
  position: relative;
  display: block;
  width: 100%;
  height: 2.8rem;
  color: #222;
  line-height: 2.8rem;
  font-size: 0.8rem;
}
.coupons-box .cou-main li dd h4 b {
  position: absolute;
  top: 0;
  right: 0;
  width: 16%;
  height: 1.7rem;
  background: url("/static/image/coupons_button_tick_press3.png") 50% no-repeat;
  -webkit-background-size: 50% 50%;
  -moz-background-size: 50% 50%;
  -ms-background-size: 50% 50%;
  -o-background-size: 50% 50%;
  background-size: 50% 50%;
}
.coupons-box .cou-main li dd h4 .b-hook {
  background: url("/static/image/coupons_button_tick_press.png") 50% no-repeat;
  -webkit-background-size: 50% 50%;
  -moz-background-size: 50% 50%;
  -ms-background-size: 50% 50%;
  -o-background-size: 50% 50%;
  background-size: 50% 50%;
}
.coupons-box .cou-main li dd strong {
  display: block;
  padding :0.3rem 0;
  color: #666;
  font-size: 0.75rem;
}
.coupons-box .cou-main li dd span {
  display: block;
  color: #666;
  font-size: 0.6rem;
}
.coupons-box .cou-main li dd a {
  position: absolute;
  top: 2.75rem;
  right: 2.29%;
  width: 20%;
  height: 1.35rem;
  border-radius: 1rem;
  background: #28c081;
  color: #fff;
  font-size: 0.65rem;
  line-height: 1.35rem;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.coupons-box .cou-main .coiling-01 {
  background: url('/static/image/coiling_color_bg_01.png') no-repeat;
  -webkit-background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  -ms-background-size: 100% 100%;
  -o-background-size: 100% 100%;
  background-size: 100% 100%;
}
.coupons-box .cou-main .coiling-02 {
  background: url('/static/image/coiling_color_bg_02.png') no-repeat;
  -webkit-background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  -ms-background-size: 100% 100%;
  -o-background-size: 100% 100%;
  background-size: 100% 100%;
}
.coupons-box .cou-main .coiling-03 {
  background: url('/static/image/coiling_color_bg_03.png') no-repeat;
  -webkit-background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  -ms-background-size: 100% 100%;
  -o-background-size: 100% 100%;
  background-size: 100% 100%;
}
.coupons-box .cou-main .check-pref {
  width: 100%;
  height: 2rem;
}
.coupons-box .cou-main .check-pref a {
  display: block;
  color: #28c081;
  font-size: 0.75rem;
  text-align: center;
}
.coupons-box .cou-main .receive-pref {
  width: 100%;
  height: 1.9rem;
}
.coupons-box .cou-main .receive-pref a {
  display: block;
  width: 90%;
  height: 1.9rem;
  margin: 0 auto;
  border-radius: 1rem;
  background: #28c081;
  color: #fff;
  line-height: 1.9rem;
  font-size: 0.9rem;
  text-align: center;
}
.coupons-box .cou-main .nothing {
  display: block;
  width: 100%;
  height: auto;
  margin-top: 1rem;
}
.coupons-box .cou-main .nothing p {
  width: 100%;
  color: #d2d2d2;
  font-size: 0.55rem;
  text-align: center;
}
.coupons-box .show {
  display: block;
}
.coupons-box .cou-main-bg {
  min-height: 85vh;
  background: #f6f6f6 url('/static/image/shopping_trolley_02.png') 50% 25% no-repeat;
  -webkit-background-size: 40% 35%;
  -moz-background-size: 40% 35%;
  -ms-background-size: 40% 35%;
  -o-background-size: 40% 35%;
  background-size: 40% 35%;
}
.coupons-box .use-main li dd i {
  position: absolute;
  top: 1.5rem;
  right: 2%;
  width: 31%;
  height: 4.3rem;
  background: url("/static/image/use_img_01.png") 50% no-repeat;
  -webkit-background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  -ms-background-size: 100% 100%;
  -o-background-size: 100% 100%;
  background-size: 100% 100%;
}
.coupons-box .invalid-main li dd i {
  position: absolute;
  top: 2rem;
  right: 2%;
  width: 31%;
  height: 3.3rem;
  background: url("/static/image/use_img_02.png") 50% no-repeat;
  -webkit-background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  -ms-background-size: 100% 100%;
  -o-background-size: 100% 100%;
  background-size: 100% 100%;
}
.coupons-tips {
  position: fixed;
  top: 0;
  width: 100%;
  min-width: 320px;
  max-width: 750px;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.coupons-tips .tips-box {
  overflow: hidden;
  position: absolute;
  top: 11.25rem;
  left: 10%;
  width: 80%;
  border-radius: 0.3rem;
  background: #fff;
}
.coupons-tips .tips-box h1 {
  width: 100%;
  height: 2.65rem;
  color: #666;
  line-height: 2.65rem;
  font-size: 1rem;
  text-align: center;
}
.coupons-tips .tips-box p {
  width: 78%;
  margin: 0 auto 0.8rem;
  color: #666;
  font-size: 0.8rem;
}
.coupons-tips .tips-box .confirm-btn {
  display: block;
  width: 100%;
  height: 2.5rem;
  background: #28c081;
  color: #fff;
  line-height: 2.5rem;
  font-size: 0.9rem;
  text-align: center;
}
